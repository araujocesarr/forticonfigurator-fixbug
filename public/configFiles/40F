config firewall ssl-ssh-profile
    edit "sdag_standard_cert-ssl-cert-inspect"
        config ssl
            set inspect-all certificate-inspection
        end
        config https
        end
        config ftps
        end
        config imaps
        end
        config pop3s
        end
        config smtps
        end
        config ssh
            set ports 22
            set status disable
        end
    next
end
