config antivirus settings
    set grayware enable
end
config antivirus profile
    edit "sdag_standard_antivirus"
        config http
            set options scan
            set outbreak-prevention full-archive
        end
        config ftp
            set options scan
            set outbreak-prevention full-archive
        end
        config imap
            set options scan
            set outbreak-prevention full-archive
        end
        config pop3
            set options scan
            set outbreak-prevention full-archive
        end
        config smtp
            set options scan
            set outbreak-prevention full-archive
        end
        config nntp
            set outbreak-prevention full-archive
        end
        config cifs
            set options scan
            set outbreak-prevention full-archive
        end
        config outbreak-prevention
            set ftgd-service enable
        end
    next
end
